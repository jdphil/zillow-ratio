{
  "manifest_version": 3,
  "name": "Zillow Save Ratio",
  "description": "Shows Views-to-Saves ratio on Zillow listings.",
  "version": "1.0",
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.zillow.com/*/*_zpid/"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["activeTab"]
}
